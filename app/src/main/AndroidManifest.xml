<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/jswicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/jswicon"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/colorPrimary"
        tools:targetApi="31">
        <activity
            android:name=".Forgot_Password.Forgot_Password"
            android:exported="false" />
        <activity
            android:name=".Homepage.category"
            android:exported="false" />
        <activity
            android:name=".Login_and_Register_Activity.Register_Activity"
            android:exported="false" />
        <activity
            android:name=".Login_and_Register_Activity.Login_Screen"
            android:exported="false" />
        <activity
            android:name=".SplashActivity.JSW_Splash_Screen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity.MainActivity"
            android:exported="true" />
    </application>

</manifest>